{
  "compilerOptions": {
    "outDir": "./build",
    "target": "es2022",
    "module": "node16",
    "lib": [
      "es2023"
    ],
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ]
  }
}